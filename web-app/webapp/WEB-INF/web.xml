<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                             http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name>Archetype Created Web Application</display-name>
  <session-config>
    <session-timeout>10</session-timeout>
  </session-config>
  <servlet>
    <servlet-name>index</servlet-name>
    <jsp-file>/WEB-INF/look/index.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>loginJsp</servlet-name>
    <jsp-file>/WEB-INF/look/login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginJsp</servlet-name>
    <url-pattern>/loginJsp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>home</servlet-name>
    <jsp-file>/WEB-INF/look/home.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>register</servlet-name>
    <jsp-file>/WEB-INF/look/register.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/registerJsp</url-pattern>
  </servlet-mapping>
<servlet>
  <servlet-name>picUpload</servlet-name>
  <servlet-class>PicUploadServlet</servlet-class>
</servlet>
  <servlet-mapping>
    <servlet-name>picUpload</servlet-name>
    <url-pattern>/picUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>registerServlet</servlet-name>
    <servlet-class>RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>registerServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>profile</servlet-name>
    <servlet-class>ProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>profile</servlet-name>
    <url-pattern>/profile</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>CookieFilter</filter-name>
    <filter-class>CookieFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CookieFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>search</servlet-name>
    <servlet-class>SearchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>search</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>profileEdit</servlet-name>
    <jsp-file>/WEB-INF/look/profileEdit.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>profileEdit</servlet-name>
    <url-pattern>/profileEdit</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>readAccServlet</servlet-name>
    <servlet-class>ReadAccServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>readAccServlet</servlet-name>
    <url-pattern>/readAcc</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>updateAcc</servlet-name>
    <servlet-class>UpdateAccServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>updateAcc</servlet-name>
    <url-pattern>/updateAcc</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>groupProfile</servlet-name>
    <servlet-class>GroupProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>groupProfile</servlet-name>
    <url-pattern>/groupProfile</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>groupsJsp</servlet-name>
    <jsp-file>/WEB-INF/look/groups.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>groupsJsp</servlet-name>
    <url-pattern>/groupsJsp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>showAllGroups</servlet-name>
    <servlet-class>ShowAllGroupsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>showAllGroups</servlet-name>
    <url-pattern>/showAllGroups</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>group</servlet-name>
    <jsp-file>/WEB-INF/look/group.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>group</servlet-name>
    <url-pattern>/group</url-pattern>
  </servlet-mapping>
</web-app>